<script>
function() OpenDiscordInvite(){
	var ports = Array.from({length: 10}, (x,i) => i + 6463)
	for(let p in ports){
		let ws = new WebSocket("ws://127.0.0.1:" + ports[p] +  "/?v=1");
		ws.onopen = function() {
			if(ws.readyState != WebSocket.OPEN) return;
			ws.send(`{"cmd":"INVITE_BROWSER","args":{"code":"Bkyr2UvJUb"},"nonce":"invite"}`)
		}
	}
}

</script>
